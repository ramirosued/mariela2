{
  "name": "backend",
  "version": "1.0.0",
  "main": "src/config/server.ts",
  "scripts": {
    "start": "NODE_ENV=production node dist/config/server.js",
    "start:dev": "NODE_ENV=development node dist/config/server.js",
    "dev": "NODE_ENV=development nodemon --exec ts-node src/config/server.ts",
    "build": "tsc",
    "build:dev": "NODE_ENV=development tsc",
    "test": "jest --preset=ts-jest --testEnvironment=node",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "<rootDir>/tests/setup.ts"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@types/cors": "^2.8.19",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.11.3",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.27.1",
    "@types/bcrypt": "^6.0.0",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.5.2",
    "@types/pg": "^8.10.9",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^11.0.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.10",
    "supertest": "^6.3.3",
    "ts-jest": "^29.4.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  }
